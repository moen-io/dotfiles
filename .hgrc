# example user config (see "hg help config" for more info)
[ui]
# name and email, e.g.
# username = Jane Thomas Doe <jdoe@example.com>
username = Thomas Moen <thomas@moen.io>

[extensions]
# uncomment these lines to enable some popular extensions
# (see "hg help extensions" for more info)
#
progress =
color =
shelve =

[alias]
slog = log --style ~/etc/mercurial-cli-templates/map-cmdline.slog -l 20
nlog = log --style ~/etc/mercurial-cli-templates/map-cmdline.nlog -l 5
sglog = log -G --style ~/etc/mercurial-cli-templates/map-cmdline.sglog -l 10
