[ui]
username = Thomas Moen <thomas@moen.io>

[extensions]
color =
shelve =
convert =
purge =
fsmonitor =

[alias]
slog = log --style ~/.config/mercurial-cli-templates/map-cmdline.slog -l 20
nlog = log --style ~/.config/mercurial-cli-templates/map-cmdline.nlog -l 5
sglog = log -G --style ~/.config/mercurial-cli-templates/map-cmdline.sglog -l 10

